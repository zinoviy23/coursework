        <th>
          +
        </th>
        <th>
          0
        </th>
        <th>
          1
        </th>
        <th>
          2
        </th>
      </tr>
      <tr>
        <td>
          <b>0</b>
        </td>
        <td>
          0
        </td>
        <td>
          1
        </td>
        <td>
          2
        </td>
      </tr>
      <tr>
        <td>
          <b>1</b>
        </td>
        <td>
          1
        </td>
        <td>
          2
        </td>
        <td>
          0
        </td>
      </tr>
      <tr>
        <td>
          <b>2</b>
        </td>
        <td>
          2
        </td>
        <td>
          0
        </td>
        <td>
          1
        </td>
      </tr>
    </table>
  </p>

